<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="service-amq" owner="taylor" organization="com.taylor" />

	<dubbo:provider serialization="kryo" codec="dubbo" server="netty" client="netty" threadpool="cached" connections="1" loadbalance="consistenthash" cluster="failfast" delay="-1" timeout="5000" />

	<dubbo:registry protocol="zookeeper" address="192.168.186.128:2181" />

	<dubbo:monitor protocol="registry" />

	<dubbo:protocol name="dubbo" serialization="kryo" port="20552" />

	<dubbo:service interface="com.taylor.api.amq.service.MessageSenderService" ref="queueMessageSenderImpl" group="queue" protocol="dubbo" delay="-1" version="0.0.1" />
	
	<dubbo:service interface="com.taylor.api.amq.service.MessageSenderService" ref="topicMessageSenderImpl" group="topic" protocol="dubbo" delay="-1" version="0.0.1" />
</beans>